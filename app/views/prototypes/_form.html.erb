<%= form_with model: @prototype, local: true do |f|%>
  <div class="field">
    <%= f.label :prototype_name, "プロトタイプの名称" %><br />
    <%= f.text_field :prototype_name, id: "prototype_title" %>
  </div>

  <div class="field">
    <%= f.label :catch_copy, "キャッチコピー" %><br />
    <%= f.text_area :catch_copy, class: "form__text", id: "prototype_catch_copy" %>
  </div>

  <div class="field">
    <%= f.label :concept_info, "コンセプト" %><br />
    <%= f.text_area :concept_info, class: "form__text", id: "prototype_concept" %>
  </div>

  <div class="field">
    <%= f.label :image_url, "プロトタイプの画像" %><br />
    <%= f.file_field :image_url, id: "prototype_image" %>
  </div>

  <div class="actions">
    <%= f.submit "保存する", class: :form__btn  %>
  </div>
<% end %>
